 Date(1900, 0, 1),\n            maxDate: new Date(),\n            currentDate: new Date(this.value)\n        };\n    },\n    props:['value'],\n    methods: {\n        async onConfirm(){\n            // 请求提交更新性别\n            try {\n                this.$toast('修改中');\n                console.log(date);\n                const date=dayjs(this.currentDate).format(\"YYYY-MM-DD\");\n                await updateUserProfile({\n                    birthday:date\n                })\n                this.$toast.success('修改成功');\n                this.$emit('input',date)\n                this.$emit('close');\n            } catch (error) {\n                this.$toast(error.message)\n            }\n        }\n    },\n}\n</script>\n\n<style lang=\"less\" scoped>\n\n</style>"]}]}